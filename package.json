{
  "name": "worm-scraper",
  "description": "Scrapes the web serial Worm into an eBook format",
  "keywords": [
    "ebook",
    "worm",
    "parahuman",
    "scraper"
  ],
  "version": "2.4.0",
  "author": "Domenic Denicola <d@domenic.me> (https://domenic.me/)",
  "license": "WTFPL",
  "repository": "domenic/worm-scraper",
  "bin": "lib/worm-scraper.js",
  "files": [
    "lib/",
    "scaffolding/",
    "npm-shrinkwrap.json"
  ],
  "scripts": {
    "lint": "eslint lib && jscs lib",
    "download": "node lib/worm-scraper.js download",
    "convert": "node lib/worm-scraper.js convert",
    "scaffold": "node lib/worm-scraper.js scaffold",
    "zip": "node lib/worm-scraper.js zip",
    "start": "npm run download && npm run convert && npm run scaffold && npm run zip"
  },
  "dependencies": {
    "archiver": "^3.1.1",
    "cpr": "^3.0.1",
    "jsdom": "^15.1.1",
    "mkdirp-then": "^1.0.1",
    "mz": "^2.7.0",
    "requisition": "^1.5.0",
    "rimraf-then": "^1.0.0",
    "thenify": "^3.3.0",
    "throat": "^5.0.0",
    "xmlserializer": "^0.6.1",
    "yargs": "^13.3.0"
  },
  "devDependencies": {
    "eslint": "6.1.0"
  }
}
